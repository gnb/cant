#!/bin/sh
#
# $Id: runtest,v 1.2 2001-11-21 16:28:07 gnb Exp $
#

. ../testfunctions.sh

fileset_test ()
{
    local VALUE="$1"
    local EXPECTED="$2"
    
    start_test "fileset"
    
    cant "test$VALUE"
    check_output_file test$VALUE.expected
}

fileset_test 1
fileset_test 1b
fileset_test 1c
fileset_test 2
fileset_test 3
fileset_test 3b
fileset_test 4
fileset_test 5
fileset_test 6
